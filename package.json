    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@expo/webpack-config": "^0.11.11",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "~5.2.1",
    "@storybook/addon-links": "~5.2.1",
